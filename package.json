  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "oldbuild": "react-scripts build && rm -rf realm_backend/MyBankGridView/hosting/files/* && cp -rv build/* realm_backend/MyBankGridView/hosting/files/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "realm:init": "npm run deploy && cd ./realm_backend && realm-cli app init",
